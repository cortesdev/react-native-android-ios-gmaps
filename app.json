{
  "expo": {
    "name": "react-native-android-ios-gmaps",
    "description": "A very interesting project.",
    "slug": "react-native-android-ios-gmaps",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
   "ios": {
      "bundleIdentifier": "cortesdev.maps",
      "supportsTablet": true
    },
    "android": {
      "package": "cortesdev.maps"
    }
  }
}
